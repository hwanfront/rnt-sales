{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "db:create": "sequelize-cli db:create --config ./config/config.js",
    "db:drop": "sequelize-cli db:drop --config ./config/config.js",
    "build": "tsc -p .",
    "dev": "nodemon src/app.ts --watch \"src/**/*.ts\" --exec \"yarn lint && npx ts-node\"",
    "lint": "eslint . --ext ts",
    "prettier": "prettier --config .prettierrc --check './src/**/*.ts'",
    "prettier:format": "prettier --config .prettierrc './src/**/*.ts' --write"
  },
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "typedi": "^0.10.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/hpp": "^0.2.2",
    "@types/method-override": "^0.0.32",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.7",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
